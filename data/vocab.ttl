@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix phoneshop: <https://example.com/phoneshop#> .

# Ontology definition
<https://example.com/phoneshop> a rdf:Ontology ;
    rdfs:comment "Ontology for the PhoneShop e-commerce site for mobile phones." ;
    rdfs:label "PhoneShop Ontology" .

# Classes
phoneshop:Horodateur a rdfs:Class ;
    rdfs:label "Horodateur" ;
    rdfs:comment "The timestamp of a record." .

phoneshop:Nom a rdfs:Class ;
    rdfs:label "Nom" ;
    rdfs:comment "The last name of a person." .

phoneshop:Prenom a rdfs:Class ;
    rdfs:label "Prénom" ;
    rdfs:comment "The first name of a person." .

phoneshop:DateDeNaissance a rdfs:Class ;
    rdfs:label "Date de naissance" ;
    rdfs:comment "The birthdate of a person." .

phoneshop:SituationPersonnel a rdfs:Class ;
    rdfs:label "Situation Personnel" ;
    rdfs:comment "The personal situation of a person (e.g., student, employee)." .

phoneshop:PaysDeResidence a rdfs:Class ;
    rdfs:label "Pays de résidence" ;
    rdfs:comment "The country where the person resides." .

phoneshop:LanguesParles a rdfs:Class ;
    rdfs:label "Langues parlées" ;
    rdfs:comment "The languages spoken by a person." .

phoneshop:UtilisationMobile a rdfs:Class ;
    rdfs:label "Avez-vous déjà utilisé un téléphone mobile ?" ;
    rdfs:comment "Indicates whether the person has used a mobile phone." .

phoneshop:MarquesPreferes a rdfs:Class ;
    rdfs:label "Quelles sont vos marques de téléphones préférées ?" ;
    rdfs:comment "The preferred mobile phone brands of a person." .

phoneshop:BudgetAchat a rdfs:Class ;
    rdfs:label "Quel budget déprécieriez-vous pour l'achat d'un téléphone ?" ;
    rdfs:comment "The budget the person would allocate for buying a mobile phone." .

phoneshop:InteretSiteEcommerce a rdfs:Class ;
    rdfs:label "Seriez-vous intéressé par un site web sécurisé dédié au e-commerce de téléphones mobiles ?" ;
    rdfs:comment "Indicates whether the person is interested in a secure e-commerce site for mobile phones." .

phoneshop:ProfilUtilisateur a rdfs:Class ;
    rdfs:label "Vous avez le profil d'un :" ;
    rdfs:comment "The user profile based on their preferences and usage." .

phoneshop:FonctionnalitesDesires a rdfs:Class ;
    rdfs:label "Fonctionnalités désirées" ;
    rdfs:comment "The desired features or functionalities for a mobile phone." .

# Properties
phoneshop:hasTimestamp a rdf:Property ;
    rdfs:label "has timestamp" ;
    rdfs:domain phoneshop:Horodateur ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Links to the timestamp of the record." .

phoneshop:hasLastName a rdf:Property ;
    rdfs:label "has last name" ;
    rdfs:domain phoneshop:Nom ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the last name of a person." .

phoneshop:hasFirstName a rdf:Property ;
    rdfs:label "has first name" ;
    rdfs:domain phoneshop:Prenom ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the first name of a person." .

phoneshop:hasBirthdate a rdf:Property ;
    rdfs:label "has birthdate" ;
    rdfs:domain phoneshop:DateDeNaissance ;
    rdfs:range xsd:date ;
    rdfs:comment "Links to the birthdate of a person." .

phoneshop:hasPersonalSituation a rdf:Property ;
    rdfs:label "has personal situation" ;
    rdfs:domain phoneshop:SituationPersonnel ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the personal situation of a person." .

phoneshop:residesIn a rdf:Property ;
    rdfs:label "resides in" ;
    rdfs:domain phoneshop:PaysDeResidence ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the country where the person resides." .

phoneshop:speaksLanguages a rdf:Property ;
    rdfs:label "speaks languages" ;
    rdfs:domain phoneshop:LanguesParles ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the languages spoken by a person." .

phoneshop:hasUsedMobilePhone a rdf:Property ;
    rdfs:label "has used mobile phone" ;
    rdfs:domain phoneshop:UtilisationMobile ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates whether the person has used a mobile phone." .

phoneshop:prefersBrands a rdf:Property ;
    rdfs:label "prefers brands" ;
    rdfs:domain phoneshop:MarquesPreferes ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the preferred mobile phone brands." .

phoneshop:hasBudget a rdf:Property ;
    rdfs:label "has budget" ;
    rdfs:domain phoneshop:BudgetAchat ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Links to the budget for mobile phone purchase." .

phoneshop:isInterestedInEcommerce a rdf:Property ;
    rdfs:label "is interested in e-commerce" ;
    rdfs:domain phoneshop:InteretSiteEcommerce ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates interest in a secure e-commerce site." .

phoneshop:hasUserProfile a rdf:Property ;
    rdfs:label "has user profile" ;
    rdfs:domain phoneshop:ProfilUtilisateur ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the user profile category." .

phoneshop:desiresFeatures a rdf:Property ;
    rdfs:label "desires features" ;
    rdfs:domain phoneshop:FonctionnalitesDesires ;
    rdfs:range xsd:string ;
    rdfs:comment "Links to the desired features of a mobile phone." .